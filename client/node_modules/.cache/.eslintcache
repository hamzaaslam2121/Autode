={{ color:'white', fontStyle: 'italic' }}>\n\t\t\t\t\t\tBy <a style={{ color: 'white' }} href={postBaseUrl} target='_blank' rel='noopener'>{postDomain}</a>\n\t\t\t\t\t</span><br/>\n\t\t\t\t\t<span style={{ color: 'white' }}>Added on {dateString}</span>\n\t\t\t\t</div>\n\t\t\t</Popover.Body>\n\t\t</Popover>\n\t)\n\n\treturn (\n\t\t<OverlayTrigger trigger={['hover', 'focus']}\n\t\t                placement='auto'\n\t\t                overlay={renderPopup}\n\t\t                show={showPopover}\n\t\t                style={{ touchAction:'none' }}>\n\t\t\t<MarkerDiv verified={post.verified}\n\t\t\t           className={'marker' + post.id}\n\t\t\t           onMouseEnter={() => setShowPopover(true)}\n\t\t\t           onMouseLeave={() => setShowPopover(false)}/>\n\t\t</OverlayTrigger>\n\t)\n}\n\nexport default Marker","/Users/hamzaaslam/VSCode/autode/client/src/components/greenwich_center.js",["85"],[],"export default { lat: 51.4769, lng: 0.0005 };","/Users/hamzaaslam/VSCode/autode/client/src/components/Maptemp.jsx",["86","87"],[],"import React, { useMemo } from 'react'\nimport { GoogleMap, Marker, useLoadScript } from '@react-google-maps/api';\nimport GoogleMapReact from 'google-map-react';\n\nimport './map.css'\n\nconst Map = () => {\n    \n  const { isLoaded } = useLoadScript({\n    googleMapsApiKey: process.env.AIzaSyB_gADBq2dJuxcRKDwv8_N1b7CAFaQ5IY8,\n  });\n  const center = useMemo(() => ({ lat: 51.4769, lng: 0.0005 }), []);\n\n    const defaultProps = {\n      center: {\n        lat: 51.4769,\n        lng: 0.0005\n      },\n      zoom: 11\n      }\n\n    const onLoad = (map) => {\n        const bounds = new window.google.maps.LatLngBounds();\n        markers?.forEach(({ lat, lng }) => bounds.extend({ lat, lng }));\n        map.fitBounds(bounds);\n      };\n    \n    const markers = [{ lat: 51.4769, lng: 0.0005 },\n                    { lat: 51.4769, lng: 0.0005 },\n                    { lat: 51.4769, lng: 0.0005 }];\n    \n\n     \n    return (\n      <div className